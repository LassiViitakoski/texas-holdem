{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "moduleResolution": "Bundler",
    "outDir": "dist",
    "skipLibCheck": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "noUncheckedIndexedAccess": true,
    "module": "ESNext",
    "noEmit": true
  },
  "include": ["src/**/*"]
}